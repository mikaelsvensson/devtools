<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
    <skin>
        <groupId>info.mikaelsvensson.doctools</groupId>
        <artifactId>doctools-skin</artifactId>
        <version>1.0-SNAPSHOT</version>
    </skin>
    <custom>
        <siteIndexerPlugin>
            <searchboxContainerInsertat>&lt;div id="navcolumn"&gt;</searchboxContainerInsertat>
            <searchboxContainerInsertaction>append</searchboxContainerInsertaction>
            <searchboxContainerId>search-form</searchboxContainerId>
            <searchboxButtonLabel>Search</searchboxButtonLabel>
            <searchboxTitleLabel>Search Site</searchboxTitleLabel>
            <searchboxIndexExcludecommonwords>true</searchboxIndexExcludecommonwords>
            <searchboxIndexExcludenumbers>false</searchboxIndexExcludenumbers>
            <searchboxIndexMinwordlength>3</searchboxIndexMinwordlength>
            <searchboxIndexExcludefolder>apidocs</searchboxIndexExcludefolder>
        </siteIndexerPlugin>
    </custom>
    <body>
        <head>
            <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
        </head>
        <menu name="Group">
            <item name="A Simple Document" href="simple.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
</project>
